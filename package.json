    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-jest": "^21.22.1",
    "jest": "^23.6.0",
    "redux-mock-store": "^1.5.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/index.js",
